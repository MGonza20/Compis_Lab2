digraph {
	A -> B [label=a dir=forward]
	A -> C [label=b dir=forward]
	A -> D [label=c dir=forward]
	B -> B [label=a dir=forward]
	B -> D [label=c dir=forward]
	C -> C [label=b dir=forward]
	C -> D [label=c dir=forward]
	A [color=green shape=circle style=filled]
	B
	C
	D [shape=doublecircle]
	rankdir=LR
}
