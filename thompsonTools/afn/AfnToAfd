digraph {
	A -> B [label=a dir=forward]
	A -> C [label=b dir=forward]
	B -> C [label=b dir=forward]
	C -> D [label=a dir=forward]
	D -> D [label=a dir=forward]
	D -> E [label=c dir=forward]
	A [color=green shape=circle style=filled]
	B
	C
	D [shape=doublecircle]
	E [shape=doublecircle]
	rankdir=LR
}
