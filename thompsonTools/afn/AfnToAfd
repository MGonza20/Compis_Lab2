	A -> B [label=a dir=forward]
	A -> C [label=b dir=forward]
	B -> D [label=b dir=forward]
	C -> B [label=a dir=forward]
	C -> C [label=b dir=forward]
	D -> E [label=b dir=forward]
	E -> F [label=a dir=forward]
	E -> G [label=b dir=forward]
	F -> F [label=a dir=forward]
	F -> G [label=b dir=forward]
	G -> F [label=a dir=forward]
	G -> G [label=b dir=forward]
	A [color=green shape=circle style=filled]
	B
	C
	D
	E [shape=doublecircle]
	F [shape=doublecircle]
	G [shape=doublecircle]
	rankdir=LR
}
