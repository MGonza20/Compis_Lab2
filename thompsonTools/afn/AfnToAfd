digraph {
	A -> B [label=a dir=forward]
	B -> C [label=a dir=forward]
	B -> D [label=b dir=forward]
	C -> E [label=b dir=forward]
	D -> C [label=a dir=forward]
	D -> D [label=b dir=forward]
	E -> E [label=b dir=forward]
	A [color=green shape=circle style=filled]
	B
	C [shape=doublecircle]
	D
	E [shape=doublecircle]
	rankdir=LR
}
